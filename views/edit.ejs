<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="utf-8" />
    <title>CubeS | <%= titulo %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%- include('./partials/doc') %>
  </head>

  <body class="grey darken-3">
    <%- include('./partials/header', { u: usuario, admin: admin}) %>
    <div class="container p-4">
      <div class="row">
        <div class="col-md-4 offset-md-4">
          <div class="card animated fadeInDown">
            <img
              src="../cloud/<%= suministro.filename %>"
              class="card-img-top"
              alt="<%= suministro.filename %>"
            />
            <div class="card-body">
              <h5 class="card-title">
                # <%= suministro.codeNumber %>
              </h5>
              <p class="card-text">
                user: <%= suministro.nameUser %>
              </p>
              <p></p>
              <a
                href="/image/<%= suministro.idSuministro %>/delete"
                class="btn btn-danger btn-block"
              >
                Delete Image
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
